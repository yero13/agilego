{
	"version": 0.1,
	"dataset": {
		"index-notimplemented": "key",
		"modify-fields": {
			"estimate": "estimate/3600"
		}
	},
	"indexes": {
		"parents": {
			"l1key": "parent",
			"l2key": "key"
		},
		"blockers": {
			"l1key": "blocked_by",
			"l2key": "key"
		},
		"blocked": {
			"l2key": "blocked_by",
			"l1key": "key"
		},
		"priorities": {
			"where": "type == 'Story' or type == 'Bug'",
			"l2key": "priority",
			"l1key": "key"
		},
		"backlog": {
			"where": "type == 'Story' or type == 'Bug'",
			"l1key": "key",
			"l2key": "type"
		}
	},
	"validations": {
	},
	"subsets": {
		"backlog": {
			"index": "backlog",
			"join_on": "key",
			"columns": ["key", "type", "priority"],
			"order": "priority",
			"sort": ["Highest", "High", "Medium", "Low", "Lowest"]			
		},
		"subtasks": {
			"index": "parents",
			"join_on": "key",
			"columns": ["key", "parent", "type", "priority", "status"],
			"order": "priority",
			"sort": ["Highest", "High", "Medium", "Low", "Lowest"]						
		},
		"backlog-details": {
			"index": "backlog",
			"join_on": "key",
			"columns": ["key", "type", "priority", "status", "assignee_id", "duedate", "estimate", "components", "blocked_by"]
		}
	}
}