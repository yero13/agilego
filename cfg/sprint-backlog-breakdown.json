{
	"version": 0.1,
	"dataset": {
		"index-notimplemented": "key",
		"modify-fields": {
			"estimate": "estimate/3600"
		}
	},
	"indexes": {
		"parents": {
			"l1key": "parent",
			"l2key": "key"
		},
		"blockers": {
			"l1key": "blocked_by",
			"l2key": "key"
		},
		"blocked": {
			"l2key": "blocked_by",
			"l1key": "key"
		},
		"priorities": {
			"where": "type == 'Story' or type == 'Bug'",
			"l2key": "priority",
			"l1key": "key",
			"order": "priority",
			"sort": ["Highest", "High", "Medium", "Low", "Lowest"]
		}
	},
	"validations": {
	},
	"subsets": {
		"backlog": {
			"index": "priorities",
			"columns": ["key", "type", "priority"]
		},
		"subtasks": {
			"index": "parents",
			"columns": ["key", "type", "priority", "status"]
		}
	}
}