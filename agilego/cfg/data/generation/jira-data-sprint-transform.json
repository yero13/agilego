{
	"version": 0.1,
	"transformation-sets": {
		"src-transform": {
			"db": {
				"src.db": "db_src_data",
				"dest.db": "db_src_data"
			},
			"transformations": {
				"set_issue_sprint": {
					"class": "transformation.transformer.MultiColDoc2XTransformation",
					"cfg": {
						"src.db.load": {
							"src.cols": ["data.issues"],
							"src.docs": ["data.sprint"]							
						},
						"transform": {
							"func": "transformation.transformer.update_col",
							"params": {
								"target": "data.issues", 
								"update": [
									{
										"src.type": "doc",
										"src.col": "data.sprint",
										"src.field": "id",
										"dest.field": "sprint_id"
									}
								]
							}						
						},
						"dest.db.cleanup": {
							"target": "data.issues"
						},
						"dest.db.save": {
							"dest": "data.issues"
						}						
					}
				}
			}
		}
	}
}