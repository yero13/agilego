{
	"version": 0.1,
	"mapping": {
		"url": "$jira_url"
	},
	"requests": {
		"create_users": {
			"cfg": "./cfg/data/generation/jira-data-create-user.json",
			"type": "create_entity",
			"src.collection": "test.users",
			"dynamic_mapping": {
				"name": "name",
				"displayName": "displayName",
				"password": "password",
				"emailAddress": "emailAddress"
			}
		},	
		"create_project": {
			"cfg": "./cfg/data/generation/jira-data-create-project.json",
			"type": "create_entity",
			"src.collection": "test.project",
			"dynamic_mapping": {
				"key": "key",
				"name": "name",
				"projectTypeKey": "projectTypeKey",
				"projectTemplateKey": "projectTemplateKey",
				"lead": "lead"
			},
			"callback.update_src": {
				"id": "id"
			}
		}
	},
	"db": "$db_src_data"
}