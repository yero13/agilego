{
	"version": 0.1,
	"transformation-sets": {
		"src-transform": {
			"db": {
				"src.db": "db_src_data",
				"dest.db": "db_src_data"
			},
			"transformations": {
				"set_sprint_board": {
					"class": "natrix.transformation.transformer.MultiDoc2XTransformation",
					"cfg": {
						"src.db.load": {
							"src": ["data.sprint", "data.board"]
						},
						"transform": {
							"func": "natrix.transformation.transformer.update_doc",
							"params": {
								"source": [ 
									{
										"src.col": "data.board",
										"src.field": "board_id"
									}
								],
								"result": "data.sprint"
							}
						},
						"dest.db.cleanup": {
							"target": "data.sprint"
						},
						"dest.db.save": {
							"dest": "data.sprint"
						}
					}				
				}
			}
		}
	}
}