{
	"version": 0.1,
	"request": {
		"url": "$url/rest/api/2/user/assignable/search",
		"data": {
			"project": "$project",
			"maxResults": 100,
			"startAt": 0
		}
	},
	"response": {
		"employees": {
			"noname": "True",
			"type": "array",
			"fields": {
				"employee": {
					"type": "object",
					"explicit": "True",
					"noname": "True",
					"fields": {
						"name": {
							"key": "name",
							"type": "string"
						},
						"displayName": {
							"key": "displayName",
							"type": "string"
						}
					}
				}
			}
		}
	}
}