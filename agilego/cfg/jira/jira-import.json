{
	"version": 0.1,
	"mapping": {
		"url": "__set_JIRA_url__here__",
		"project": "__set_project__here__",
		"sprint": 1
	},
	"requests": {
		"components": {
			"cfg": "./cfg/jira/jira-components-request.json",
			"type": "list",
			"dest": "components"		
		},
		"employees": {
			"cfg": "./cfg/jira/jira-team-request.json",
			"type": "list",
			"dest": "employees"		
		},		
		"backlog": {
			"cfg": "./cfg/jira/jira-sprint-backlog-request.json",
			"type": "list",
			"dest": "backlog"
		},
		"sprint": {
			"cfg": "./cfg/jira/jira-sprint-definition-request.json",
			"type": "single_object",
			"dest": "sprint"		
		}		
	},
	"db": "jira-import"
}