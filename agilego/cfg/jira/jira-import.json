{
	"version": 0.1,
	"mapping": {
		"url": "$jira_url",
		"project": "$scrum_project",
		"sprint": "$scrum_sprint",
		"field_agilego_tech": "$field_agilego_tech" 
	},
	"requests": {
		"components": {
			"cfg": "./cfg/jira/jira-components-request.json",
			"type": "list",
			"dest": "components"		
		},
		"employees": {
			"cfg": "./cfg/jira/jira-team-request.json",
			"type": "list",
			"dest": "employees"		
		},		
		"backlog": {
			"cfg": "./cfg/jira/jira-sprint-backlog-request.json",
			"type": "list",
			"dest": "backlog"
		},
		"sprint": {
			"cfg": "./cfg/jira/jira-sprint-definition-request.json",
			"type": "single_object",
			"dest": "sprint"		
		}		
	},
	"db": "$db_jira_import"
}