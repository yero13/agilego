			"db": {
				"src.db": "db_jira_import",
				"dest.db": "db_scrum_transform"
			},
			"transformations": {
				"components": {
					"class": "framework.transformation.transformer.Col2XTransformation",
					"cfg": {
						"src.db.load": {
							"src": "components"
						},
						"transform": {
							"func": "framework.transformation.transformer.group_singles2array",
							"params": {
								"field.single": "component",
								"field.array": "component"
							}							
						},
						"dest.db.cleanup": {
							"target": "project.components"
						},
						"dest.db.save": {
							"dest": "project.components"
						}
					}
				},
				"employees": {
					"class": "framework.transformation.transformer.Col2XTransformation",
