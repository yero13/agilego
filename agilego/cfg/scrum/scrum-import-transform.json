		"int-transform": {
			"db": {
				"src.db": "db_scrum_transform",
				"dest.db": "db_scrum_transform"
			},
			"transformations": {
				"all_items_filtered": {
					"class": "framework.transformation.transformer.Col2XTransformation",
					"cfg": {
						"src.db.load": {
							"src": "sprint.all_items"
						},
						"transform": {
							"func": "framework.transformation.transformer.filter_set",
							"params": {
								"where": "type == 'Story' or type == 'Bug' or type == 'Sub-task'"
							}
						},
						"dest.db.cleanup": {
							"target": "sprint.all_items_filtered"
						},
						"dest.db.save": {
							"dest": "sprint.all_items_filtered"
						}
					}
				},
				"all_items_sorted": {
					"class": "framework.transformation.transformer.Col2XTransformation",
					"cfg": {
						"src.db.load": {
							"src": "sprint.all_items_filtered"
						},
						"transform": {
							"func": "framework.transformation.transformer.sort_set",
							"params": {
								"sort.field": "priority",
								"sort.order": ["Highest", "High", "Medium", "Low", "Lowest"]							
							}
						},
						"dest.db.cleanup": {
							"target": "sprint.all_items_sorted"
						},
						"dest.db.save": {
